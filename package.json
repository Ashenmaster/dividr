{
  "name": "dividr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently 'node ./bin/www' 'npm start --prefix ./client/'",
    "build": "cd client && webpack -p",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc mocha tests/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "engines": {
    "node": "7.0.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "concurrently": "^3.4.0",
    "cookie-parser": "~1.4.3",
    "crypto": "0.0.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jsonwebtoken": "^7.4.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.6",
    "morgan": "~1.8.1",
    "newrelic": "^1.38.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "codecov": "^2.1.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.2.0",
    "supertest": "^3.0.0"
  }
}
